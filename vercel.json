{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/dist/$1"
        }
    ],
    "redirects": [
        {
            "source": "/frontend/",
            "destination": "/"
        }
    ],
    "functions": {
        "api/*.py": {
            "runtime": "python3.12"
        }
    }
}