{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ],
    "redirects": [
        {
            "source": "/frontend/",
            "destination": "/"
        }
    ],
    "functions": {
        "api/index.py": {
            "runtime": "@vercel/python"
        }
    }
}